{
    "label": "{{ label }}",
    "name": "unyc_trunk_{{ tenant_uuid }}_{{ suffix }}",
    "transport": {"uuid": "{{ transport_uuid }}"},
    "templates": [{"uuid": "{{ global_sip_template_uuid }}"}, {"uuid":"{{ registration_trunk_sip_template_uuid }}"}],
    "endpoint_section_options": [
        ["contact_user", "{{ username }}"],
        ["identify_by", "header,username"],
        ["from_domain", "sip{{ domain }}"],
        ["outbound_proxy", "sip:sip{{ domain }}\\;lr"],
        ["allow", "!all,alaw"],
	["send_pai", "no"],
	["set_var", "PJSIP_HEADER(add,P-Asserted-Identity)=\"{{ ndi }}\" <sip:{{ ndi }}@sip{{ domain }}>"]
    ],
    "registration_section_options": [
        ["expiration", "600"],
        ["line", "yes"],
        ["client_uri", "sip:{{ username }}@{{ domain }}:5060"],
        ["server_uri", "sip:sip{{ domain }}:5060"]
    ],
    "registration_outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "aor_section_options": [
        ["contact", "sip:{{ username }}@{{ domain }}:5060"]
    ],
    "identify_section_options": [
        ["match_header", "Contact: /<sip:{{ username }}@.*>/"],
        ["match", "{{ domain }}"]
    ]
}
