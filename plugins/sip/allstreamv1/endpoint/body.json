{
    "label": "{{ label }}",
    "name": "allstream_trunk_{{ tenant_uuid }}_{{ suffix }}",
    "transport": {"uuid": "{{ transport_uuid }}"},
    "templates": [{"uuid":"{{ global_sip_template_uuid }}"}, {"uuid":"{{ registration_trunk_sip_template_uuid }}"}],
    "endpoint_section_options": [
        ["identify_by", "header,auth_username,username"],
        ["from_user", "{{ username }}"],
        ["rewrite_contact", "yes"],
        ["send_rpid", "yes"],
        ["allow", "!all,ulaw"]
    ],
    "registration_section_options": [
        ["client_uri", "sip:{{ username }}@sip.allstream.com:5060"],
        ["server_uri", "sip:sip.allstream.com:5060"]
    ],
    "registration_outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "outbound_auth_section_options": [
        ["username", "{{ username }}"],
        ["password", "{{ password }}"]
    ],
    "aor_section_options": [
        ["contact", "sip:{{ username }}@sip.allstream.com"]
    ],
    "identify_section_options": [
        ["match", "sip.allstream.com"]
    ]
}
